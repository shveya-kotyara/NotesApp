new Date(Date.now()).toLocaleString(),\n      });\n\n      this.note.title = \"\";\n      this.note.descr = \"\";\n\n      this.message = null;\n    },\n    removeNote(index) {\n      this.notes.splice(index, 1);\n    },\n  },\n};\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src"}]}